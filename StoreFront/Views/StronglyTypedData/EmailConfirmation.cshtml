@model StoreFrontLabMVC.UI.MVC.Models.ContactViewModel

@{
    ViewBag.Title = "Email Confirmation";
}

<h2>@ViewBag.Title</h2>


<h3>Thank you @Model.Name!</h3>
<p>
    We have received your email, and will respond soon-ish. Details of your message are below.
</p>

<p class="text-success">
    Name: @Model.Name
    <br />
    Email: @Model.Email
    <br />
    Subject: @Model.Subject
    <br />
    Message: @Model.Message
</p>